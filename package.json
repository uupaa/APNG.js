{
  "name": "uupaa.png.js",
  "version": "0.0.2",
  "description": "PNG and APNG implementation.",
  "url": "https://github.com/uupaa/PNG.js",
  "keywords": ["PNG", "APNG", "decoder", "uupaa", "WebModule", "Unstable"],
  "repository": {
    "type": "git",
    "url": "https://github.com/uupaa/PNG.js.git"
  },
  "scripts": {
    "sync": "node ../WebModule/run/sync.js; npm update; node ../WebModule/run/page.js",
    "min": "node ../WebModule/run/minify.js --verbose --strict --keep --pretty",
    "build": "node ../WebModule/run/minify.js --verbose --strict --keep --release",
    "watch": "node ../WebModule/run/watch.js --verbose --run hint",
    "page": "node ../WebModule/run/page.js --verbose",
    "test": "npm run min; npm run node; npm run browser",
    "node": "NODE_ENV=production NODE_PATH=lib node --harmony test/node.js",
    "noded": "NODE_ENV=production NODE_PATH=lib node --debug-brk --harmony test/node.js",
    "browser": "open http://localhost:8000/PNG.js/test/index.html",
    "sim": "node ../WebModule/run/sim.js http://localhost:8000/PNG.js/test/index.html",
    "simx": "node ../WebModule/run/sim.js stop",
    "hint": "eslint lib/*.js",
    "score": "node ../WebModule/run/score.js; open lint/plato/index.html",
    "patch": "node ../WebModule/run/patch.js",
    "setup": "node ../WebModule/run/setup.js",
    "start": "cd ../; Python -m SimpleHTTPServer 8000 &",
    "stop": "killall -- Python -m SimpleHTTPServer 8000; exit 0"
  },
  "webmodule": {
    "develop": false,
    "label": ["@dev"],
    "browser": {
      "source": ["lib/PNG.js"],
      "output": "release/PNG.b.min.js"
    },
    "worker": {
      "source": ["lib/PNG.js"],
      "output": "release/PNG.w.min.js"
    },
    "node": {
      "source": ["lib/PNG.js"],
      "output": "release/PNG.n.min.js"
    },
    "nw": {
      "source": ["lib/PNG.js"],
      "output": "release/PNG.nw.min.js"
    }
  },
  "dependencies": {
    "uupaa.hash.js": ""
  },
  "devDependencies": {},
  "lib": "./lib/",
  "main": "./index.js",
  "author": "uupaa",
  "license": "MIT",
  "contributors": []
}